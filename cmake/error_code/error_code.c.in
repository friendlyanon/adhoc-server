#include <stdbool.h>

#include "error_code.h"

static const int values[] = {
    @values@,
};

bool is_ah_error_code(int value)
{
  const int* begin = values;
  const int* const end = &values[sizeof(values) / sizeof(int)];
  do {
    if (*begin == value) {
      return true;
    }
  } while (++begin != end);

  return false;
}
